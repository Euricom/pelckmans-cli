{
  "compilerOptions": {
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "bin",
    "baseUrl":".",
    "rootDir": "src",
    "strict": true,
    "target": "es2017",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    },
  },
  "include": ["src/**/*"]
}
